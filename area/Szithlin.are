> speech_prog p I'm ready for the fight!~
mpquest $n drowarena 1
if quest(drowarena) == 1
  mppause 3
  if level($n) > 9
    mpmload 2223
    mpechoat $n &cA drow wimp staggers in, cowering at the noises.&D
  else
    mpechoat $n &cYou are not strong enough to fight this beast.
  endif
else
  if quest(drowarena) == 3
    if level($n) > 19
      mpmload 2224
      mpechoat $n &cA vicious lion jumps into the arena.&D
      mppause 3
    else
      mpechoat $n You are not strong enough for this beast.
    endif
  else
    if quest(drowarena) ==5
      if level($n) > 29
        mpmload 2225
        mpechoat $n A Horrible malboro slides into the arena.
        mppause 3
      else
        mpechoat $n You are not strong enough for this beast.
      endif
    else
      if quest(drowarena) == 7
        if level($n) > 39
          mpmload 2226
          mpechoat $n A fersome basilisk slithers towards you.
          mppause 3
        else
          mpechoat $n You are not strong enough for this beast.
        endif
      else
        if quest(drowarena) == 9
          if level($n) > 49
            mpmload 2227
            mpechoat $n A Hecatonchires stalks into the arena.
            mppause 3
          else
            mpechoat $n You are not strong enough for this beast.
          endif
        else
          if quest(drowarena) == 11
            if level($n) > 59
              mpmload 2228
              mpechoat $n A drow elite silently glides into the arena.
              mppause 3
            else
              mpechoat $n You are not strong enough for this beast.
            endif
          else
            if quest(drowarena) == 13
              if level($n) > 69
                mpmload 2229
                mpechoat $n Cerberus charges into the arena, snarling.
                mppause 3
              else
                mpechoat $n You are not strong enough for this beast.
              endif
            else
              if quest(drowarena) == 15
                if level($n) > 79
                  mpmload 2230
                  mpechoat $n An elder wyrm slides into the arena.
                  mppause 3
                else
                  mpechoat $n You are not strong enough for this beast.
                endif
              else
                if quest(drowarena) == 17
                  if level($n) > 89
                    mpmload 2231
                    mpechoat $n A giant hydra sways back and forth, as it makes it's way towards you.
                    mppause 3
                  else
                    mpechoat $n You are not strong enough for this beast.
                  endif
                else
                  if quest(drowarena) == 19
                    if level($n) > 99
                      mppause 3
                      mpmload 2232
                      mpechoat $n The golden soul reaver dragon roars with an earsplitting screetch.
                    else
                      mpechoat $n You are not strong enough for this beast.
                    endif
                  endif
                endif
              endif
            endif
          endif
        endif
      endif
    endif
  endif
endif
